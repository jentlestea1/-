<global name="global">
  <template_data_list>
    <template_data type="plain_struct" name="tdm0">
      <para_list length="1">
	    <para name="slave_address" type="char">0xD2</para>
      </para_list>
    </template_data>
  </template_data_list>
</global>

<op_list length="4">
  <op name="gyroscope_open" template_id="0">
    <template_data_list>
      <template_data type="reg_array" name="tdm0">
        <para_list length="5">
          <para address="0x20">0x00</para>
          <para address="0x21">0x00</para>
          <para address="0x22">0x08</para>
          <para address="0x23">0x03</para>
          <para address="0x24">0x00</para>				  
        </para_list>
      </template_data>
    </template_data_list>
  </op>

  <op name="gyroscope_getx" template_id="0">
    <template_data_list>
      <template_data type="plain_struct" name="tdm0">
        <para_list length="2">
          <para name="reg_address" type="char">0x28</para> 
          <para name="size" type="int">2</para>
        </para_list>				    
      </template_data>
    </template_data_list>
  </op>

  <op name="gyroscope_gety" template_id="0">
    <template_data_list>
      <template_data type="plain_struct" name="tdm0">
        <para_list length="2">
          <para name="reg_address" type="char">0x2c</para> 
          <para name="size" type="int">2</para>
        </para_list>
      </template_data>
    </template_data_list>
  </op>

  <op name="gyroscope_getz" template_id="0">
    <template_data_list>
      <template_data type="plain_struct" name="tdm0">
        <para_list length="2">
          <para name="reg_address" type="char">0x2f</para> 
          <para name="size" type="int">2</para>
        </para_list>
      </template_data>
    </template_data_list>
  </op>
</op_list>
